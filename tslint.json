{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-snake-case"
    ],
    "interface-name": [true, "never-prefix"],
    "no-console": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
